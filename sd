
# GRADE of a student
sub1 = 75
sub2 = 60
sub3 = 91
sub4 = 78
sub5 = 88
total = sub1 + sub2 + sub3 +sub4 + sub5
p = (total/500)*100
if 90 <= p <= 100:
                   print("A*")
elif 80 <= p <= 89: 
                    print("A")
elif 70 <= p <= 79:
                   print("B")
elif 60 <= p <= 69:
                   print("C")
elif 50 <= p <= 59:
                   print("D")
else:
     print("F")